  <file source-language="en" datatype="plaintext" original="ng2.template" target-language="es">
    <body>
      <!-- Device Configuration: Afinion -->

      <trans-unit id="DeviceConfigurations.Afinion.DeviceSettingsTitle">
          <source>Settings</source>
          <target>Configuración</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.AllowLocal">
          <source>Allow Local</source>
          <target>Permitir Local</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.Operator.AllowLocal.Tooltip">
          <source>Allow local operator lockout configuration</source>
          <target>Permite la configuración de desbloqueo del operador local</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.AssayLockout.Tooltip">
          <source>If checked, will block the instrument from performing patient tests for the assay type</source>
          <target>Si se selecciona, bloqueará al instrumento para realizar pruebas a pacientes del tipo de ensayo</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.Assay.LockoutInterval.TooltipTime">
          <source>Number of hours between required control runs</source>
          <target>Número de horas entre las ejecuciones de control requeridas</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.Assay.LockoutInterval.TooltipRuns">
          <source>Number of runs between required control runs</source>
          <target>Número de series entre las series de control requeridas</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.Assay.WarningPeriod.TooltipTime">
          <source>Number of hours remaining to show warning</source>
          <target>Número de horas restantes para mostrar la advertencia</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.Assay.WarningPeriod.TooltipRuns">
          <source>Number of runs remaining to show warning</source>
          <target>Número de carreras restantes para mostrar la advertencia</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.QC.AllowLocal.Tooltip">
          <source>Allow local QC lockout configuration</source>
          <target>Permitir configuración de bloqueo de calidad en el lugar</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.SessionDuration">
          <source>Session Duration</source>
          <target>Duración de la sesión</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.LockoutLevel">
          <source>Lockout Level</source>
          <target>Nivel de Bloqueo</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.OperatorLockoutTitle">
          <source>Operator Lockout</source>
          <target>Bloqueo del operador</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.QCLockoutTitle">
          <source>QC Lockout</source>
          <target>Bloqueo de QC</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.LockoutLevel.Disabled">
          <source>Disabled</source>
          <target>Inhabilitado</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.LockoutLevel.Required">
          <source>Required</source>
          <target>Obligatorio</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.LockoutLevel.IdVerification">
          <source>Required with ID Verification</source>
          <target>Requisito con verificación de ID</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.LockoutBy.Time">
          <source>Time</source>
          <target>Tiempo</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.LockoutBy.Run">
          <source>Runs</source>
          <target>Carreras</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.LockoutLevel.Off">
          <source>Off</source>
          <target>Desactivado</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.LockoutLevel.CIOrCII">
          <source>CI or CII</source>
          <target>CI o CII</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.LockoutLevel.CIAndCII">
          <source>CI and CII</source>
          <target>CI y CII</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.Columns.AssayLockout">
          <source>Assay Lockout</source>
          <target>Bloqueo de ensayos</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.Columns.AssayName">
          <source>Assay Name</source>
          <target>Nombre del ensayo</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.Columns.LockoutBy">
          <source>Lockout By</source>
          <target>Bloqueo Por</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.Columns.LockoutInterval">
          <source>Lockout Interval</source>
          <target>Intervalo de bloqueo</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.Columns.WarningPeriod">
          <source>Warning Period</source>
          <target>Período de Advertencia</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.Assay.Columns.LockoutLevel">
          <source>Lockout Level</source>
          <target>Nivel de Bloqueo</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.OperatorCapabilitiesTitle">
          <source>Operator Capabilities</source>
          <target>Habilidades del operador</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.AllowDiscard">
          <source>Allow Discard</source>
          <target>Permitir Descartar</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.Operator.AllowDiscard.Tooltip">
          <source>Tooltip for Afinion Allow Discard checkbox</source>
          <target>Allow user to discard unwanted results</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.FirmwareOptionsTitle">
          <source>Firmware Update Options</source>
          <target>Opciones de actualización del firmware</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.AutoUpdate">
          <source>Automatic Update</source>
          <target>Actualización automática</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.AutoUpdate.Tooltip">
          <source>Tooltip for Afinion Automatic Update checkbox</source>
          <target>The firmware update will be performed with no user interaction required.</target>
      </trans-unit>
      <trans-unit id="DeviceConfigurations.Afinion.AutoRestart">
          <source>Automatic Restart</source>
          <target>Restauración automática</target>
      </trans-unit>
      <trans-unit id="DeviceConfiguration.Afinion.AutoRestart.Tooltip">
          <source>Tooltip for Afinion Automatic Restart checkbox</source>
          <target>The Afinion will restart automatically after uploading the firmware update.</target>
      </trans-unit>
    </body>
